@model UserViewModel

@{
    ViewBag.Title = "Edit a User";
}

<form asp-controller="Users" asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    @await Html.PartialAsync("_CreateOrEdit")
    <button class="btn btn-primary">Update</button>
    <a asp-controller="Users" asp-action="Index">Cancel</a>
</form>