@model UserViewModel

@{ViewBag.Title="Edit User";}

<form asp-controller="Users" asp-action="Edit" method="post">
    <div>
        <label asp-for="FirstName"></label> : <input type="text" asp-for="FirstName"/>
        <span asp-validation-for="FirstName"></span>
    </div>

    <div>
        <label asp-for="LastName"></label> : <input type="text" asp-for="LastName"/>
        <span asp-validation-for="LastName"></span>
    </div>

    <button>Save</button>
</form>